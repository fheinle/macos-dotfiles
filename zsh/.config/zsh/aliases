alias ..="cd .."
alias aws-profile='export AWS_PROFILE=$(sed -n "s/\[profile \(.*\)\]/\1/gp" ~/.aws/config | fzf)'
alias cert="nmap -p443 --script ssl-cert"
alias g="rg --smart-case --context 1"
alias k="kubectl"
alias l="exa --grid --classify --icons --color=auto --all --sort name"
alias ls="exa --header --long --classify --icons --color=auto --all --sort modified --time-style iso"
alias tree="exa --tree -I .git --all --long --header --classify"
alias vi="nvim"
# vim: set ft=zsh
